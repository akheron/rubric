{
  "compilerOptions": {
    "strict": true,
    "target": "es5",
    "lib": ["esnext", "dom", "dom.iterable"],
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "sourceMap": true,
    "jsx": "react",
    "esModuleInterop": true,
    "baseUrl": "./src",
    "noImplicitAny": true,
    "outDir": "./dist/"
  }
}
